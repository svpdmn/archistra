# Required
OPENROUTER_API_KEY=your_openrouter_api_key
AUTH0_SECRET=replace_with_long_random_value
AUTH0_BASE_URL=http://localhost:3000
AUTH0_ISSUER_BASE_URL=https://YOUR_TENANT.us.auth0.com
APP_BASE_URL=http://localhost:3000
AUTH0_DOMAIN=https://YOUR_TENANT.us.auth0.com
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret

# Optional
OPENROUTER_MODEL=deepseek/deepseek-r1-0528:free
OPENROUTER_TIMEOUT_MS=20000
OPENROUTER_MAX_OUTPUT_TOKENS=500
OPENROUTER_TEMPERATURE=0.3
OPENROUTER_SITE_URL=http://localhost:3000
OPENROUTER_APP_NAME=archistra
AUTH0_AUDIENCE=
AUTH0_SCOPE=openid profile email
AUTH0_LOGOUT_REDIRECT_URL=/
AUTH0_CLAIMS_NAMESPACE=https://archistra.app

# Rate limit settings (per IP)
CHAT_RATE_LIMIT_MAX_REQUESTS=30
CHAT_RATE_LIMIT_WINDOW_MS=60000
CHAT_AUTH_BYPASS=false
